{
  "name": "pdf-lib-up",
  "version": "2.0.0",
  "description": "Create and modify PDF documents in any JS/TS environment",
  "license": "MIT",
  "private": false,
  "author": "Andrew Dillon <andrew.dillon.j@gmail.com>",
  "contributors": [
